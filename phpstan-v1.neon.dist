parameters:
    level: 5
    paths:
        - src
        - config
    tmpDir: build/phpstan
    ignoreErrors:
        - '#Called ''env'' outside of the config directory#'
        - '#Call to an undefined static method Omise.*\:\:request\(\)\.$#'
        - '#Call to an undefined static method OmiseApiResource\:\:setCurlOptions\(\)\.$#'
        - '#Property .* is never read, only written\.$#'